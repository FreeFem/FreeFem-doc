real[int] xx(nn), yy(nn);
for (int i = 0; i <= n; i++) {
	xx[i] = real(i)/n;
	yy[i] = u(0.5, real(i)/n);	// value of uh at point (0.5, i/10.)
}
plot([xx(0:n), yy(0:n)], wait=1);

// End
assert(err == 0);

