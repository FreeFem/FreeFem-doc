func complex[int] CPREC1(complex[int] &u) 
{
  complex[int] res(u.n);
  res(0:FVhi.ndof-1) = FPREC1(u(0:FVhi.ndof-1));
  res(FVhi.ndof:Cn-1) = (-1)*BPREC1(u(FVhi.ndof:Cn-1));
