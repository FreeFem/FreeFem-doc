        MatCreate(Th1, B, Pk);
        MatCreate(Th0, A[0], Pk);
        MatInterpolate(Th1, Pk, B, Th0, Pk, A[0], P[0]);
        MatInterpolate(Th2, Pk, C, Th1, Pk, B, P[1]);
    }

    real[int] rhs;
    varf vPb(u, v) = int2d(Th0)(grad(u)' * grad(v)) + int2d(Th0)(v) + on(1, u = 0.0);
    fespace Wh(Th0, Pk);
    A[0] = vPb(Wh, Wh, tgv = -2);
