        - int3d(Th)(k^2*[vx,vy,vz]'*[Ex,Ey,Ez])
        - int2d(Th, Robin)(1i*k*CrossN(vx,vy,vz)'*CrossN(Ex,Ey,Ez))
        - int2d(Th, fakeInterface)(1i*k*CrossN(vx,vy,vz)'*CrossN(Ex,Ey,Ez))
        + on(Dirichlet, Ex=0, Ey=0, Ez=0);
    Opt = vPbOpt(Wh, Wh);
}
DDM(A, rhs, nEx, O = Opt);
Wh<complex> def(sol);
